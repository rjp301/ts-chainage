{
  "name": "chainage",
  "version": "1.0.2",
  "description": "Geospatial library with a focus on working with the centerlines of linear projects",
  "main": "./dist/cjs/src/index.js",
  "module": "./dist/esm/src/index.js",
  "type": "commonjs",
  "files": [
    "dist/cjs/src",
    "dist/esm/src"
  ],
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest",
    "tsc": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "tsc-watch": "tsc -p tsconfig.json --watch",
    "prepublishOnly": "npm run tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rjp301/ts-chainage.git"
  },
  "keywords": [
    "GIS",
    "geospatial",
    "chainage"
  ],
  "author": "Riley Paul",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rjp301/ts-chainage/issues"
  },
  "homepage": "https://github.com/rjp301/ts-chainage#readme",
  "dependencies": {
    "@types/geojson": "^7946.0.10"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/jest": "^29.5.2",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0"
  }
}
